{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/anchors": "3.4.0",
    "craftcms/aws-s3": "2.2.1",
    "craftcms/ckeditor": "4.3.1",
    "craftcms/cms": "5.4.9",
    "nystudio107/craft-imageoptimize": "5.0.3",
    "nystudio107/craft-seomatic": "5.1.5",
    "nystudio107/craft-vite": "5.0.1",
    "putyourlightson/craft-blitz": "5.9.1",
    "spicyweb/craft-embedded-assets": "5.2.0",
    "topshelfcraft/wordsmith": "^3.0",
    "verbb/formie": "3.0.9",
    "verbb/navigation": "3.0.5",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}