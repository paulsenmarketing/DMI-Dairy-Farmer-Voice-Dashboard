{% extends 'index.twig' %}

{% block body %}

   {% set blocks = entry.contentBlocks.all() %}

   {% for block in blocks %}
      {% set camel = block.type.handle %}
      <p>{{ camel }}:</p>
      {% include("_templates/blocks/#{camel}.twig") with {'block': block} only %}
   {% endfor %}


{% endblock body %}